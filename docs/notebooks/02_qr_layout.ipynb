      "source": [
        "# QR layout\n",
        "Render a small QR grid as SVG."
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "import sympy as sym\n",
        "from IPython.display import SVG, display\n",
        "from matrixlayout.qr import qr_grid_svg\n",
        "\n",
        "A = sym.Matrix([[1, 2], [3, 4]])\n",
        "W = sym.eye(2)\n",
        "matrices = [[None, None, A, W]]\n",
        "svg = qr_grid_svg(matrices=matrices)\n",
        "display(SVG(svg))\n"
      ]
    }
  ],
